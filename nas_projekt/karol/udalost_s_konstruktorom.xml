		<event type="finish">
			<id>e_konstruktor</id>
			<actions phase="post">
				<action>
					postihnuteOsoby: f.postihnuta_osoba,
					id: f.id_poistky;

					def casePoistka = findCase({ it._id.eq(new org.bson.types.ObjectId(id.value)) })
					def data = getData("120",casePoistka)

					for ( i in data["ludia"].choices) {
						change postihnuteOsoby choices{
							return postihnuteOsoby.choices + i;
						}
					}
				</action>
			</actions>
		</event>
	</transition>
