		<dataGroup>
			<id>platba_group</id>
			<title>Platobné informácie</title>
			<dataRef>
				<id>ucet_poukazka</id>
				<logic>
					<behavior>editable</behavior>
					<action trigger="set">
						meno: f.meno_poukazka,
						priezvisko: f.priezvisko_poukazka,
						ulica: f.ulica_poukazka,
						cisloUl: f.cislo_ul_poukazka,
						psc: f.psc_poukazka,
						mesto: f.mesto_poukazka,
						ucetCislo: f.cislo_ucet,
						ucetSwift: f.swift_kod_ucet,
						ucetMajitel: f.majitel_ucet,
						flag: f.this,
						info: t.404;

						if(flag.value == true) {
						make meno,required on info when { true }
						make meno,editable on info when { true }

						make priezvisko,required on info when { true }
						make priezvisko,editable on info when { true }

						make ulica,required on info when { true }
						make ulica,editable on info when { true }

						make cisloUl,required on info when { true }
						make cisloUl,editable on info when { true }

						make psc,required on info when { true }
						make psc,editable on info when { true }

						make mesto,required on info when { true }
						make mesto,editable on info when { true }

						make ucetCislo,hidden on info when { true }
						make ucetCislo,optional on info when { true }

						make ucetSwift,hidden on info when { true }
						make ucetSwift,optional on info when { true }

						make ucetMajitel,hidden on info when{ true }
						make ucetMajitel,optional on info when { true }


						change ucetCislo value { null }
						change ucetSwift value { null }
						change ucetMajitel value { null }
						}
						else {
						make ucetCislo,required on info when { true }
						make ucetCislo,editable on info when { true }

						make ucetSwift,required on info when { true }
						make ucetSwift,editable on info when { true }

						make ucetMajitel,required on info when { true }
						make ucetMajitel,editable on info when { true }

						make meno,hidden on info when { true }
						make meno,hidden on info when { true }

						make priezvisko,hidden on info when { true }
						make priezvisko,optional on info when { true }

						make ulica,hidden on info when { true }
						make ulica,optional on info when { true }

						make cisloUl,hidden on info when { true }
						make cisloUl,optional on info when { true }

						make psc,hidden on info when { true }
						make psc,optional on info when { true }

						make mesto,hidden on info when { true }
						make mesto,optional on info when { true }

						change meno value { null }
						change priezvisko value { null }
						change ulica value { null }
						change cisloUl value { null }
						change psc value { null }
						change mesto value { null }
						}
					</action>
				</logic>
			</dataRef>
		</dataGroup>
