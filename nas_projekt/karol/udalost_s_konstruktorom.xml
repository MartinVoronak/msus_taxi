	<transition>
		<id>416</id>
		<x>580</x>
		<y>20</y>
		<label>Kalkulácia [systém]</label>
		<roleRef>
			<id>system</id>
			<logic>
				<perform>true</perform>
			</logic>
		</roleRef>
		<dataRef>
			<id>id_poistky</id>
			<logic>
				<behavior>visible</behavior>
				<action trigger="set">
					kalk: f.kalkulacia;
					suma: f.suma_dokumenty;
					ramec: f.poistny_ramec;

					change kalk value {
						if(suma.value > ramec.value) {
							return suma.value;
						}
						else {
							return ramec.value
						}
					}
				</action>
			</logic>
		</dataRef>
	</transition>
